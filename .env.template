# Iterpro API Configuration
# Copy this file to .env and fill in your actual credentials

# Base URL for the Iterpro API
BASE_URL=https://api.iterpro.com/api/v1

# Your Iterpro API key
# Get this from your Iterpro dashboard at https://api.iterpro.com/
API_KEY=your_iterpro_api_key_here

# Authorization header (HTTP Basic Authentication)
# Format: Basic base64(username:password)
# Example: Basic dXNlcm5hbWU6cGFzc3dvcmQ=
# To generate: echo -n "username:password" | base64
AUTH_HEADER=Basic your_base64_encoded_username_password_here
